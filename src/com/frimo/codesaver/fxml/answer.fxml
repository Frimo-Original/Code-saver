<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <Text fx:id="answer" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
    <Button fx:id="buttonOk" prefWidth="50.0" text="Ok" GridPane.columnIndex="0" GridPane.columnSpan="1" GridPane.rowIndex="1" />
    <Button fx:id="buttonNo" prefWidth="50.0" text="No" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button fx:id="buttonCancel" text="Cancel" GridPane.columnIndex="2" GridPane.rowIndex="1" />

   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
